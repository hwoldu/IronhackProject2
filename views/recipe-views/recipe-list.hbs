<section class="home">

      <h2>All {{season}} recipes</h2>
      <section>

            <section class="recipes">

                  {{#each recipeSchema}}
                  <div class="polaroid">
                    <a href="/recipe-one/{{this._id}}"><p>{{this.title}}</p>
                    <img src="{{this.image}}" alt=""/></a>
                  </div>
                  {{/each}}

            </section>
      </section>
</section>